<h1>Todos <%= link_to "New Todo", new_todo_path, class: "btn btn-primary todo-button" %>
</h1>
<% @todos.each do |todo| %>
  <div class="todo-index">
	  <div class="<%= 'cross-out' if todo.complete == true %>">
	  <h4><%= link_to todo.name, todo_path(todo) %></h4>
	  Due Date: <%= todo.due_date %><br>

	  Complete: <%= todo.complete == true ? todo.complete = "Yes" : todo.complete = "No" %>
    
	  </div>
  </div>
<% end %> 



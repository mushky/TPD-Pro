<div class="col-md-5">
	<h2>Closed</h2>
	<div id='todo-form'>
	  <% @todos.each do |todo| %>
	    <% if todo.complete == true %>
	      <div class='todo-index'>
		      <div class='<%= 'cross-out' if todo.complete == true %>'>
			      <h5 id='todo'>
			    	  <%= link_to todo.name, todo_path(todo) %>
			      </h5>

				    <div id='todo'>
				    	Due Date: <%= todo.due_date %><br>
				    </div>

				    <div id='todo'>
				    	Complete: <%= todo.complete == true ? todo.complete = 'Yes' : todo.complete = 'No' %>
				    </div>
			      <div id='todo'>
			    	  <%= link_to 'Edit', edit_todo_path(todo), remote: true %> |
			    	  <%= link_to 'Delete', @todo, method: :delete, :data => {:confirm => 'Are you sure?'} %>
			      </div>
		      </div>
	      </div>
	    <% end %>
	  <% end %> 
	</div>
</div>